//Процедура заполняет признаки возможен экспорт по умолчанию в документах ЭДВС
Процедура ЗаполнитьПустыеПризнакиЭкспорт(ДанныеДляЗаполненияПЭ, ДокументОбъект) Экспорт
	
	//При заполнении вытягиваем данные из табличной части документа и заполнение идёт только для строк с заполненным полем "Признак происхождения"
	Запрос = Новый Запрос;
	Запрос.Текст = 
	"ВЫБРАТЬ
	|	Товары.НомерСтроки КАК НомерСтроки,
	|	Товары.Товар КАК Товар,
	|	Товары.КодТНВЭД КАК КодТНВЭД,
	|	Товары.ТипПошлины КАК ТипПошлины,
	|	Товары.ПризнакПроисхождения КАК ПризнакПроисхождения,
	|	Товары.ИмяТЧ КАК ИмяТЧ
	|ПОМЕСТИТЬ вт_Товар
	|ИЗ
	|	&ТоварИзТЧ КАК Товары
	|;
	|
	|////////////////////////////////////////////////////////////////////////////////
	|ВЫБРАТЬ
	|	вт_Товар.НомерСтроки КАК НомерСтроки,
	|	вт_Товар.Товар.Представление КАК ТоварПредставление,
	|	вт_Товар.КодТНВЭД КАК КодТНВЭД,
	|	вт_Товар.ТипПошлины КАК ТипПошлины,
	|	вт_Товар.ПризнакПроисхождения КАК ПризнакПроисхождения,
	|	вт_Товар.ИмяТЧ КАК ИмяТЧ,
	|	ВЫБОР
	|		КОГДА вт_Товар.ПризнакПроисхождения = ""2""
	|				ИЛИ вт_Товар.ПризнакПроисхождения = ""3""
	|				ИЛИ вт_Товар.ПризнакПроисхождения = ""4""
	|				ИЛИ вт_Товар.ПризнакПроисхождения = ""1""
	|					И (вт_Товар.ТипПошлины = ЗНАЧЕНИЕ(Перечисление.ТипыПошлинВС.ЕТТ)
	|						ИЛИ вт_Товар.ТипПошлины = ЗНАЧЕНИЕ(Перечисление.ТипыПошлинВС.ЕАЭС))
	|			ТОГДА ИСТИНА
	|		ИНАЧЕ ВЫБОР
	|				КОГДА вт_Товар.ПризнакПроисхождения = ""5""
	|						ИЛИ вт_Товар.ПризнакПроисхождения = ""1""
	|							И вт_Товар.ТипПошлины = ЗНАЧЕНИЕ(Перечисление.ТипыПошлинВС.ВТО)
	|					ТОГДА ЛОЖЬ
	|			КОНЕЦ
	|	КОНЕЦ КАК ВозможенЭкспорт
	|ИЗ
	|	вт_Товар КАК вт_Товар";
	
	Запрос.УстановитьПараметр("ТоварИзТЧ", ДанныеДляЗаполненияПЭ);
	Запрос.УстановитьПараметр("Дата", ДокументОбъект.Дата);
	
	Результат = Запрос.Выполнить();

	Если НЕ Результат.Пустой() Тогда
		ВыборкаДетальныеЗаписи = Результат.Выбрать();
		Пока ВыборкаДетальныеЗаписи.Следующий() Цикл
			ТекСтрока = ДокументОбъект[ВыборкаДетальныеЗаписи.ИмяТЧ][ВыборкаДетальныеЗаписи.НомерСтроки-1];
			Если ВыборкаДетальныеЗаписи.ПризнакПроисхождения <> "" Тогда
				ТекСтрока.ВозможенЭкспорт = ВыборкаДетальныеЗаписи.ВозможенЭкспорт;
			Иначе
				ТекстОшибки = ЭСФКлиентСерверПереопределяемый.ПодставитьПараметрыВСтроку(НСтр("ru = 'Для товара ""%1"" отсутствуют данные по графе ""Признак происхождения"".
				|Графа ""Возможен экспорт"" не заполнена.'"),ВыборкаДетальныеЗаписи.ТоварПредставление);
				ЭСФКлиентСерверПереопределяемый.СообщитьПользователю(ТекстОшибки, ДокументОбъект.Ссылка, ОбщегоНазначенияКлиентСервер.ПутьКТабличнойЧасти(ВыборкаДетальныеЗаписи.ИмяТЧ, ВыборкаДетальныеЗаписи.НомерСтроки, "ПризнакПроисхождения"),"Объект",)
			КонецЕсли;
		КонецЦикла;
	КонецЕсли;

