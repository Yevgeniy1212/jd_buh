Процедура ПроверитьИсходящиеЭАВР(Знач МассивИсходящихЭАВР) Экспорт
	
	Если ТипЗнч(МассивИсходящихЭАВР) <> Тип("Массив") Тогда
		Возврат;
	КонецЕсли;
	
	Для Каждого СсылкаЭАВР Из МассивИсходящихЭАВР Цикл
		РезультатПроверки = ПроверитьАВР(СсылкаЭАВР);
		ТекстСообщения = ТекстСообщенияПоРезультатуПроверкиЭСФ(СсылкаЭАВР, РезультатПроверки);
		Если НЕ ПустаяСтрока(ТекстСообщения) Тогда
			ЭСФКлиентСерверПереопределяемый.СообщитьПользователю(ТекстСообщения, СсылкаЭАВР);
		КонецЕсли;
	КонецЦикла;
	
КонецПроцедуры

Функция ПроверитьАВР(СсылкаАВР) Экспорт
	
	РезультатПроверки = ПустойРезультатПроверкиЭСФ();
	
	// Объект документ не записывается, он получается только для сокращения обращений к ИБ.
	ОбъектАВР = СсылкаАВР.ПолучитьОбъект();
	
	Попытка
		ПроверитьРазличныеДанныеАВР(ОбъектАВР, РезультатПроверки);
		ПроверитьИсправленныйЭАВР(ОбъектАВР, РезультатПроверки);
	Исключение
		
		ИнформацияОбОшибке = ИнформацияОбОшибке();
		ПодробноеПредставлениеОшибки = ПодробноеПредставлениеОшибки(ИнформацияОбОшибке);
		
		ТекстСообщения = ЭСФКлиентСерверПереопределяемый.ПодставитьПараметрыВСтроку(НСтр(
		"ru = 'При проверке заполнения ЭАВР возникла ошибка:
		|%1'"),ПодробноеПредставлениеОшибки);
		
		ЭСФКлиентСерверПереопределяемый.СообщитьПользователю(ТекстСообщения);
		
	КонецПопытки;
	
	Возврат РезультатПроверки;
	
КонецФункции

Процедура ПроверитьРазличныеДанныеАВР(ОбъектАВР, РезультатПроверки) Экспорт
	
	
	#Область РазделАОбщийРаздел
	
	Если НЕ ЗначениеЗаполнено(ОбъектАВР.ТипАВР) Тогда
		ТекстСообщения = НСтр("ru = 'А. Не заполнен ""Тип документа АВР"".'"); 
		РезультатПроверки.ЗапрещающиеОшибки.Добавить(ТекстСообщения);
	КонецЕсли;
	
	Если НЕ ЗначениеЗаполнено(ОбъектАВР.Номер) Тогда
		ТекстСообщения = НСтр("ru = 'А. Не заполнен ""Номер"" АВР учетной системы.'"); 
		РезультатПроверки.ЗапрещающиеОшибки.Добавить(ТекстСообщения);
	КонецЕсли;
	
	Если НЕ ЗначениеЗаполнено(ОбъектАВР.Дата) Тогда
		ТекстСообщения = НСтр("ru = 'А. Не заполнена ""Дата составления(выписки)"" АВР.'"); 
		РезультатПроверки.ЗапрещающиеОшибки.Добавить(ТекстСообщения);
	КонецЕсли;
	
	Если ОбъектАВР.ТипАВР = Перечисления.ТипыАВР.Исправленный Тогда
		
		Если НЕ ЗначениеЗаполнено(ОбъектАВР.НомерОсновногоАВР) И НЕ ЗначениеЗаполнено(ОбъектАВР.ДатаОсновногоАВР) И НЕ ЗначениеЗаполнено(ОбъектАВР.РегистрационныйНомерОсновногоАВР) Тогда
			
			ТекстСообщения = НСтр("ru = 'А. Не заполнена информация о связанном АВР, в который вносятся изменения.'");
			РезультатПроверки.ЗапрещающиеОшибки.Добавить(ТекстСообщения);
			
		КонецЕсли;
		
		Если НЕ ЗначениеЗаполнено(ОбъектАВР.НомерОсновногоАВР) Тогда
			
			ТекстСообщения = НСтр("ru = 'А. Не заполнен ""Номер основного АВР"".'");
			РезультатПроверки.ЗапрещающиеОшибки.Добавить(ТекстСообщения);
			
		КонецЕсли;
		
		Если НЕ ЗначениеЗаполнено(ОбъектАВР.ДатаОсновногоАВР) Тогда
			
			ТекстСообщения = НСтр("ru = 'А. Не заполнена ""Дата основного АВР"".'");
			РезультатПроверки.ЗапрещающиеОшибки.Добавить(ТекстСообщения);
			
		КонецЕсли;
		
		Если НЕ ЗначениеЗаполнено(ОбъектАВР.РегистрационныйНомерОсновногоАВР) Тогда
			
			ТекстСообщения = НСтр("ru = 'А. Не заполнен ""Регистрационный номер АВР, в который вносятся изменения"".'");
			РезультатПроверки.ЗапрещающиеОшибки.Добавить(ТекстСообщения);
			
		КонецЕсли;
		
	КонецЕсли;
	
	#КонецОбласти
	
	#Область РазделВИсполнителиПоставщики
	Если ОбъектАВР.Поставщики.Количество() <> 0 Тогда
		Если НЕ ЗначениеЗаполнено(ОбъектАВР.Поставщики[0].ТипРегистрации) Тогда
			ТекстСообщения = НСтр("ru = 'B. Не заполнен тип регистрации(категория) исполнителя АВР.'"); 
			РезультатПроверки.ЗапрещающиеОшибки.Добавить(ТекстСообщения);
		КонецЕсли;
	КонецЕсли;
	#КонецОбласти
	
	#Область РазделDЗаказчикиПолучатели
	Если ОбъектАВР.Получатели.Количество() <> 0 Тогда
		Если НЕ ЗначениеЗаполнено(ОбъектАВР.Получатели[0].ТипРегистрации) И НЕ ОбъектАВР.Получатели[0].Нерезидент Тогда
			ТекстСообщения = НСтр("ru = 'D. Не заполнен тип регистрации(категория) заказчика АВР.'"); 
			РезультатПроверки.ЗапрещающиеОшибки.Добавить(ТекстСообщения);
		КонецЕсли;
	КонецЕсли;
	#КонецОбласти
	
	#Область РазделGДанныеПоВыполненнымРаботам
	
	ПроверитьТаблицуУслуги(ОбъектАВР, РезультатПроверки);
	
	#КонецОбласти
	
КонецПроцедуры

Процедура ПроверитьТаблицуУслуги(ОбъектАВР, РезультатПроверки) Экспорт
	
	Запрос = Новый Запрос;
	
	Запрос.Текст = "ВЫБРАТЬ
	|	АВРУслуги.НомерСтроки,
	|	АВРУслуги.СоставнойКодГСВС,
	|	АВРУслуги.ДатаВыполнения
	|ПОМЕСТИТЬ ВТ_АВРУслуги
	|ИЗ
	|	&ТаблицаУслуги КАК АВРУслуги
	|;
	|
	|////////////////////////////////////////////////////////////////////////////////
	|ВЫБРАТЬ *
	|ИЗ
	|	ВТ_АВРУслуги КАК ВТ_АВРУслуги
	|";	
	
	Запрос.УстановитьПараметр("ТаблицаУслуги", ОбъектАВР.Услуги.Выгрузить());
	
	Выборка = Запрос.Выполнить().Выбрать();
	
	Пока Выборка.Следующий() Цикл
		
		Если НЕ ЗначениеЗаполнено(Выборка.СоставнойКодГСВС) Тогда
			
			ТекстСообщения = ЭСФКлиентСерверПереопределяемый.ПодставитьПараметрыВСтроку(НСтр("ru = 'G. Услуги. Не заполнен ""Составной код ГСВС"" в строке №%1.'"),Выборка.НомерСтроки); 
			РезультатПроверки.ЗапрещающиеОшибки.Добавить(ТекстСообщения);
			
		КонецЕсли;
		
		Если НЕ ЗначениеЗаполнено(Выборка.ДатаВыполнения) И НЕ ЗначениеЗаполнено(ОбъектАВР.ДатаВыполненияРабот) Тогда
			
			ТекстСообщения = ЭСФКлиентСерверПереопределяемый.ПодставитьПараметрыВСтроку(НСтр("ru = 'G. Услуги. Заполнить колонку ""Дата выполнения работ (оказания услуг)"" в строке №%1 или поле ""Дата выполнения работ (оказания услуг)"" в разделе А.'"),Выборка.НомерСтроки); 
			РезультатПроверки.ЗапрещающиеОшибки.Добавить(ТекстСообщения);
			
		КонецЕсли;
		
	КонецЦикла;
	
КонецПроцедуры

Процедура ПроверитьИсправленныйЭАВР(ОбъектАВР, РезультатПроверки) Экспорт
	
	Если ОбъектАВР.ТипАВР <> Перечисления.ТипыАВР.Исправленный Тогда
		Возврат;
	КонецЕсли;
	
	Если ОбъектАВР.СвязанныйАВР.Пустая() Тогда
		Возврат;
	КонецЕсли;
	
	// Объект основной ЭАВР не записывается, он получается только для сокращения обращений к ИБ.
	ОбъектОсновнойЭАВР = ОбъектАВР.СвязанныйАВР.ПолучитьОбъект();
	
	СравнитьОсновнойИИсправленныйЭАВР(ОбъектОсновнойЭАВР, ОбъектАВР, РезультатПроверки);
	
КонецПроцедуры

// Описание типов проверок:
// - Реквизит - проверяется совпадение значений реквизитов.
// - ТабличнаяЧасть - проверяется совпадение количества строк.
// - РеквизитТабличнойЧасти - перебираются все строки табличной части и проверяется, что указанный реквизит совпадает. 
//   Выполняется только если количество строк в табличных частях совпадает.
//
Процедура СравнитьОсновнойИИсправленныйЭАВР(Знач ОбъектОсновнойЭАВР, Знач ОбъектИсправленныйЭАВР, РезультатПроверки) Экспорт
	
	ТаблицаРазличий = Новый ТаблицаЗначений;
	ТаблицаРазличий.Колонки.Добавить("Сообщение");
	
	// Получить объекты основного и исправленного ЭАВР,
	// т.к. обращаться множество раз к реквизитам объекта,
	// может быть оптимальнее, чем к реквизитам ссылки.
	
	ТаблицаРеквизитовДляПроверки = РеквизитыДляПроверкиОсновногоИИсправленногоЭАВР();
	
	Для Каждого РеквизитДляПроверки Из ТаблицаРеквизитовДляПроверки Цикл
		
		ТипПроверки = РеквизитДляПроверки.ТипПроверки;
		ИмяТабличнойЧасти = РеквизитДляПроверки.ИмяТабличнойЧасти;
		ИмяРеквизита = РеквизитДляПроверки.ИмяРеквизита;
		СинонимТабличнойЧасти = РеквизитДляПроверки.СинонимТабличнойЧасти;
		СинонимРеквизита = РеквизитДляПроверки.СинонимРеквизита;
		СинонимРаздела = РеквизитДляПроверки.СинонимРаздела;
		
		Если ТипПроверки = ЭСФКлиентСервер.ТипПроверкиРеквизит() Тогда
			
			Если ОбъектОсновнойЭАВР[ИмяРеквизита] <> ОбъектИсправленныйЭАВР[ИмяРеквизита] Тогда
				Различие = ТаблицаРазличий.Добавить();
				Различие.Сообщение = ЭСФКлиентСерверПереопределяемый.ПодставитьПараметрыВСтроку(НСтр("ru = '%1. Отличается значение реквизита ""%2"".'"),СинонимРаздела, СинонимРеквизита);
			КонецЕсли;
			
		ИначеЕсли ТипПроверки = ЭСФКлиентСервер.ТипПроверкиТабличнаяЧасть() Тогда
			
			Если ОбъектОсновнойЭАВР[ИмяТабличнойЧасти].Количество() <> ОбъектИсправленныйЭАВР[ИмяТабличнойЧасти].Количество() Тогда
				Различие = ТаблицаРазличий.Добавить();
				Различие.Сообщение = ЭСФКлиентСерверПереопределяемый.ПодставитьПараметрыВСтроку(НСтр("ru = '%1. Отличается количество строк в табличной части ""%2"".'"),СинонимРаздела, СинонимТабличнойЧасти);
			КонецЕсли;
			
		ИначеЕсли ТипПроверки = ЭСФКлиентСервер.ТипПроверкиРеквизитТабличнойЧасти() Тогда
			
			Если ОбъектОсновнойЭАВР[ИмяТабличнойЧасти].Количество() = ОбъектИсправленныйЭАВР[ИмяТабличнойЧасти].Количество() Тогда
				
				МаксимальныйИндекс = ОбъектОсновнойЭАВР[ИмяТабличнойЧасти].Количество() - 1;
				
				Для ИндексСтроки = 0 По МаксимальныйИндекс Цикл
					
					Если ОбъектОсновнойЭАВР[ИмяТабличнойЧасти][ИндексСтроки][ИмяРеквизита] <> ОбъектИсправленныйЭАВР[ИмяТабличнойЧасти][ИндексСтроки][ИмяРеквизита] Тогда
						
						Различие = ТаблицаРазличий.Добавить();
						Различие.Сообщение = ЭСФКлиентСерверПереопределяемый.ПодставитьПараметрыВСтроку(НСтр("ru = '%1. В табличной части ""%2"", в строке №%3, отличается значение реквизита ""%4"".'"),СинонимРаздела, СинонимТабличнойЧасти, ИндексСтроки + 1, СинонимРеквизита);												
					КонецЕсли;
					
				КонецЦикла;
				
			КонецЕсли;
			
		Иначе
			
			ВызватьИсключение НСтр("ru = 'Неизвестный тип проверки:'") + " " + РеквизитДляПроверки.ТипПроверки;
			
		КонецЕсли;
		
	КонецЦикла;

	ТаблицаРазличий.Сортировать("Сообщение");
		
	Если ТаблицаРазличий.Количество() <> 0 Тогда
		
		СтрокаВсеРазличия = "";
		Для Каждого СтрокаТаблицыРазличий Из ТаблицаРазличий Цикл
			СтрокаВсеРазличия = СтрокаВсеРазличия + "- " + СтрокаТаблицыРазличий.Сообщение + Символы.ПС;
		КонецЦикла;
		
		СтрокаВсеРазличия = НСтр("ru = 'Недопустимые различия между основным и исправленным АВР:'") + Символы.ПС + СтрокаВсеРазличия;
		
		РезультатПроверки.УведомительныеОшибки.Добавить(СтрокаВсеРазличия);
		
	КонецЕсли;
	
КонецПроцедуры

Функция РеквизитыДляПроверкиОсновногоИИсправленногоЭАВР()
	
	ТаблицаРеквизитовДляПроверки = Новый ТаблицаЗначений;
	
	ТаблицаРеквизитовДляПроверки.Колонки.Добавить("ТипПроверки");
	ТаблицаРеквизитовДляПроверки.Колонки.Добавить("ИмяТабличнойЧасти");
	ТаблицаРеквизитовДляПроверки.Колонки.Добавить("ИмяРеквизита");
	ТаблицаРеквизитовДляПроверки.Колонки.Добавить("СинонимТабличнойЧасти");
	ТаблицаРеквизитовДляПроверки.Колонки.Добавить("СинонимРеквизита");
	ТаблицаРеквизитовДляПроверки.Колонки.Добавить("СинонимРаздела");
	
	Макет = ПолучитьМакет("РеквизитыДляПроверкиОсновногоИИсправленногоЭАВР");
	
	ИмяОбластиПроверкиЭАВР = "ЭАВР_Версия2";
	
	ОбластьРеквизитовДляПроверки = Макет.ПолучитьОбласть(ИмяОбластиПроверкиЭАВР);
	
	Для НомерСтроки = 1 По ОбластьРеквизитовДляПроверки.ВысотаТаблицы Цикл
		РеквизитДляПроверки = ТаблицаРеквизитовДляПроверки.Добавить();
		РеквизитДляПроверки.ТипПроверки           = СокрЛП(ОбластьРеквизитовДляПроверки.Область(НомерСтроки, 1, НомерСтроки, 1).Текст);
		РеквизитДляПроверки.ИмяТабличнойЧасти     = СокрЛП(ОбластьРеквизитовДляПроверки.Область(НомерСтроки, 2, НомерСтроки, 2).Текст);
		РеквизитДляПроверки.ИмяРеквизита          = СокрЛП(ОбластьРеквизитовДляПроверки.Область(НомерСтроки, 3, НомерСтроки, 3).Текст);
		РеквизитДляПроверки.СинонимТабличнойЧасти = СокрЛП(ОбластьРеквизитовДляПроверки.Область(НомерСтроки, 4, НомерСтроки, 4).Текст);
		РеквизитДляПроверки.СинонимРеквизита      = СокрЛП(ОбластьРеквизитовДляПроверки.Область(НомерСтроки, 5, НомерСтроки, 5).Текст);
		РеквизитДляПроверки.СинонимРаздела        = СокрЛП(ОбластьРеквизитовДляПроверки.Область(НомерСтроки, 6, НомерСтроки, 6).Текст);
	КонецЦикла;
	
	Возврат ТаблицаРеквизитовДляПроверки;
	
КонецФункции

#КонецОбласти

#КонецОбласти 

#Область Криптография

