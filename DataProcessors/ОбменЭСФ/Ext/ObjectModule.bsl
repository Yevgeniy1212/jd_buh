				СтрокаОшибки.Текст = ТекстНеУдалосьЗаполнитьДолиУчастниковСД();
			КонецЕсли;
			
		КонецЕсли;
		
	КонецЕсли;
	
	// Раздел I. Дополнительные сведения.
	
	ЭСФ.ДополнительныеСведения = ЭСФСервер.ПолучитьСвойство(Invoice, "addInf", "");
	
