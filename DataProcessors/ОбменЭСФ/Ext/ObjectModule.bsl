		КонецЕсли;
	КонецЕсли;
	
	ЗаписьЖурналаРегистрации(
	НСтр("ru = 'ОбменЭСФ.ОткрытьСессию'"), 
	УровеньЖурналаРегистрации.Информация,,,
	НСтр("ru='Идентификатор сессии: '" + ИдентификаторСессии));
	
