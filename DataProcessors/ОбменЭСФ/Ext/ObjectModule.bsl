			КонецЕсли;
			
			tin = ЭСФСервер.ПолучитьСвойство(seller, "tin", Неопределено);
			
			Если ТипЗнч(tin) = Тип("Строка") И (НЕ ПустаяСтрока(tin)) Тогда
				
