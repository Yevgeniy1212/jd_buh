	consignee = ЭСФСервер.ПолучитьСвойство(Invoice, "consignee", Неопределено);
	Если consignee <> Неопределено Тогда
		ЭСФ.ГрузополучательИдентификатор = ЭСФСервер.ПолучитьСвойство(consignee, "tin", "");
