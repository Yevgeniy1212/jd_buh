		СтрокаТаблицы.КодТНВЭД								 = ЭСФСервер.ПолучитьСвойство(Строкаupdate, "tnvedCode", "");
		СтрокаТаблицы.ДополнительныйИдентификатор			 = ЭСФСервер.ПолучитьСвойство(Строкаupdate, "productId", 0);
