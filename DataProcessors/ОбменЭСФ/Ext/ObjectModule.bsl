		ЭСФ.ПунктНазначения                  = ЭСФСервер.ПолучитьСвойство(deliveryTerm, "destination", "");
	КонецЕсли;
	
	// Раздел F. Реквизиты государственного учреждения.
	
	publicOffice = ЭСФСервер.ПолучитьСвойство(Invoice, "publicOffice", Неопределено);	
