		ПО АвансовыйОтчет.Ссылка = СуммыПоДокументам.Ссылка

ГДЕ
	АвансовыйОтчет.Дата МЕЖДУ &amp;ДатаНачала И &amp;ДатаОкончания
	И АвансовыйОтчет.Организация = &amp;Организация
	И АвансовыйОтчет.Проведен
	И ЕСТЬNULL(СуммыПоДокументам.СуммаДокумента, 0) &lt;&gt; 0
	//Доп условие</Текст>
			</Запрос>
			<Запрос Имя="ПлатежноеПоручениеИсходящее" ИспользуетсяПриЗагрузке="false">
