Если (ПустоеЗначение(Счет) = 0)  И (ПустоеЗначение(Субконто) = 0) Тогда
	Если Счет.КоличествоСубконто() &gt; 0 Тогда
		ВидСубконтоИсточника = Счет.ВидСубконто(НомерСубконто);
		ИмяПКО = "";
		ИмяПКОВидСубконто = "ВидыСубконтоТиповые";
		
		Если ТипЗначенияСтр(Субконто) = "СписокЗначений" Тогда
			Если Субконто.Получить("ВидСубконто") = "ДокументРасчетовСКонтрагентами" Тогда
				ВидСубконто = "000024"; // Партии
				ИмяПКОВидСубконто = "ВидыСубконтоТиповыеСтр";
				ИмяПКО = "ДокументРасчетовСКонтрагентом";
			КонецЕсли;
		КонецЕсли;
		
		Если (ВидСубконтоИсточника = ВидыСубконто.Контрагенты) И (ТипЗначенияСтр(Субконто) = "Справочник") Тогда
			Если ПустоеЗначение(Субконто.ИдентификационныйНомер) = 1 ТОгда
				ИмяПКО = "КонтрагентыБезИдентификационныйНомер";
			Иначе
				ИмяПКО = "Контрагенты";
			КонецЕсли;
		КонецЕсли;
		
		Если (ВидСубконтоИсточника = ВидыСубконто.Контрагенты) И (Счет = СчетПоКоду("3232")) Тогда
			//ВидСубконто = "000008"; //Работники организации
			//ИмяПКОВидСубконто = "ВидыСубконтоТиповыеСтр";
			ВидСубконто = "Сотрудники";
			ИмяПКО = "ФизЛицоКонтрагента";
