		Если ВидСубконтоИсточника = ВидыСубконто.Основания ТОгда
			Если ((Счет.ПринадлежитГруппе(СчетПоКоду("11")) = 1) И (Счет&lt;&gt;СчетПоКоду("116"))) ИЛИ ((Счет.ПринадлежитГруппе(СчетПоКоду("12")) = 1) И (НЕ(Счет.ПринадлежитГруппе(СчетПоКоду("125")) = 1)) И (Счет&lt;&gt;СчетПоКоду("1273")))
			 ИЛИ ((Счет.ПринадлежитГруппе(СчетПоКоду("20")) = 1) И (Счет&lt;&gt;СчетПоКоду("207"))) ИЛИ (Счет.ПринадлежитГруппе(СчетПоКоду("22")) = 1)
			 ИЛИ ((Счет.ПринадлежитГруппе(СчетПоКоду("21")) = 1) И (НЕ(Счет.ПринадлежитГруппе(СчетПоКоду("215")) = 1)) И (Счет&lt;&gt;СчетПоКоду("2173")))
			 ИЛИ (Счет = СчетПоКоду("291")) Тогда
				ИмяПКО = "Основания";				
			КонецЕсли;
			Если ((Счет.ПринадлежитГруппе(СчетПоКоду("30")) = 1) И (Счет&lt;&gt;СчетПоКоду("308")))
			 ИЛИ ((Счет.ПринадлежитГруппе(СчетПоКоду("33")) = 1) И (Счет&lt;&gt;СчетПоКоду("335")) И (НЕ(Счет.ПринадлежитГруппе(СчетПоКоду("338")) = 1)))
			 ИЛИ (Счет = СчетПоКоду("3381")) ИЛИ (Счет = СчетПоКоду("3382")) ИЛИ (Счет = СчетПоКоду("3383")) ИЛИ (Счет = СчетПоКоду("3387"))
			 ИЛИ (Счет = СчетПоКоду("341")) ИЛИ (Счет = СчетПоКоду("342"))
			 ИЛИ ((Счет.ПринадлежитГруппе(СчетПоКоду("35")) = 1) И (Счет&lt;&gt;СчетПоКоду("352")))
			 ИЛИ (Счет.ПринадлежитГруппе(СчетПоКоду("40")) = 1)
			 ИЛИ ((Счет.ПринадлежитГруппе(СчетПоКоду("41")) = 1) И (Счет&lt;&gt;СчетПоКоду("4164")) И (Счет&lt;&gt;СчетПоКоду("4165")) И (Счет&lt;&gt;СчетПоКоду("4166")))
			 ИЛИ (Счет = СчетПоКоду("421")) ИЛИ (Счет = СчетПоКоду("422")) ИЛИ (Счет = СчетПоКоду("441")) ИЛИ (Счет = СчетПоКоду("443")) ИЛИ (Счет = СчетПоКоду("444"))
			 ИЛИ (Счет = СчетПоКоду("161")) ИЛИ (Счет = СчетПоКоду("163")) ИЛИ (Счет = СчетПоКоду("164")) Тогда
				ИмяПКО = "Основания";
			КонецЕсли;
