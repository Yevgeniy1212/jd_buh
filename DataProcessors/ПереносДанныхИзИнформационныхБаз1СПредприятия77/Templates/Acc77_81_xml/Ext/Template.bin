				ИсходящиеДанные = СоздатьОбъект("СписокЗначений");
				ИсходящиеДанные.ДобавитьЗначение(Валюта,"ВалютаДоговора");
			КонецЕсли;
		КонецЕсли;
						
		// фиксированные активы нужно разделить на основные средства и нематериальные активы
		Если ВидСубконтоИсточника = ВидыСубконто.ФиксированныеАктивы Тогда
			//Сначала нужно понять, может быть это субконто "Внеоборотные активы"
			Если (Счет = СчетПоКоду("151")) ИЛИ (Счет = СчетПоКоду("152")) ИЛИ (Счет = СчетПоКоду("153"))
				 ИЛИ (Счет = СчетПоКоду("621")) ИЛИ (Счет = СчетПоКоду("622"))
				 ИЛИ (Счет = СчетПоКоду("741")) ИЛИ (Счет = СчетПоКоду("742")) Тогда
				ВидСубконто = "000022"; // Внеоборотные активы
				ИмяПКОВидСубконто = "ВидыСубконтоТиповыеСтр";
				Если Субконто.ВидАктива = Перечисление.ВидыФиксированныхАктивов.НематериальныйАктив Тогда
					// Биологические активы
					Если (Субконто.ГруппаФА = Перечисление.ГруппыФА.МноголетниеНасаждения) ИЛИ (Субконто.ГруппаФА = Перечисление.ГруппыФА.ПродуктивныйСкот) ИЛИ (Субконто.ГруппаФА = Перечисление.ГруппыФА.РабочийСкот) ТОгда
						ИмяПКО = "ФиксированныеАктивыБиологическиеАктивы";
					Иначе
						ИмяПКО = "ФиксированныеАктивыНематериальныеАктивы";
					КонецЕсли;
				Иначе
					ИмяПКОВидСубконто = "ВидыСубконтоТиповыеСтр";
					ИмяПКО = "ФиксированныеАктивыОсновныеСредства";
				КонецЕсли;
			Иначе
				Если Субконто.ВидАктива = Перечисление.ВидыФиксированныхАктивов.НематериальныйАктив Тогда
					// Биологические активы
					Если (Субконто.ГруппаФА = Перечисление.ГруппыФА.МноголетниеНасаждения) ИЛИ (Субконто.ГруппаФА = Перечисление.ГруппыФА.ПродуктивныйСкот) ИЛИ (Субконто.ГруппаФА = Перечисление.ГруппыФА.РабочийСкот) ТОгда
						ВидСубконто = "000028"; // Биологические активы
						ИмяПКОВидСубконто = "ВидыСубконтоТиповыеСтр";
						ИмяПКО = "ФиксированныеАктивыБиологическиеАктивы";
					Иначе
						ВидСубконто = "000029"; // Нематериальные активы
						ИмяПКОВидСубконто = "ВидыСубконтоТиповыеСтр";
						ИмяПКО = "ФиксированныеАктивыНематериальныеАктивы";
					КонецЕсли;
				Иначе
					Если Счет = СчетПоКоду("293") ТОгда
						ВидСубконто = "000016"; // ОбъектыСтроительства
						ИмяПКОВидСубконто = "ВидыСубконтоТиповыеСтр";
						ИмяПКО = "ФиксированныеАктивыОбъектыСтроительства";
					Иначе
						ВидСубконто = "000025"; // Основные средства
						ИмяПКОВидСубконто = "ВидыСубконтоТиповыеСтр";
						ИмяПКО = "ФиксированныеАктивыОсновныеСредства";
					КонецЕсли;
				КонецЕсли;
			КонецЕсли;
		КонецЕсли;
		
		Если Субконто = "Партия" Тогда
			ВидСубконто = "000023"; // Партии
			ИмяПКОВидСубконто = "ВидыСубконтоТиповыеСтр";
			ИмяПКО = "Партии";
		КонецЕсли;

		Если ВидСубконтоИсточника = ВидыСубконто.ДенежныеСредства Тогда
			Если (Счет = СчетПоКоду("101")) Тогда
				ВидСубконто = "000003"; // ДенежныеСредства
				ИмяПКОВидСубконто = "ВидыСубконтоТиповыеСтр";
				ИмяПКО = "ДенежныеСредстваКассы";
			КонецЕсли;
			Если  (Счет = СчетПоКоду("103")) ИЛИ (Счет = СчетПоКоду("104")) ИЛИ (Счет = СчетПоКоду("105")) ИЛИ (Счет = СчетПоКоду("106"))
				 ИЛИ (Счет = СчетПоКоду("107")) ИЛИ (Счет = СчетПоКоду("108")) ИЛИ (Счет = СчетПоКоду("109")) Тогда
				ВидСубконто = "000003"; // ДенежныеСредства
				ИмяПКОВидСубконто = "ВидыСубконтоТиповыеСтр";
				ИмяПКО = "ДенежныеСредстваБанковскиеСчета";
			КонецЕсли;
		КонецЕсли;
		
		Если ВидСубконтоИсточника = ВидыСубконто.Материалы Тогда
			
			ВидСубконто = "000001"; // Номенклатура
			ИмяПКОВидСубконто = "ВидыСубконтоТиповыеСтр";
			ИмяПКО = "Материалы";
			     			
		КонецЕсли;
		Если ВидСубконтоИсточника = ВидыСубконто.Номенклатура Тогда
			
			ВидСубконто = "000001"; // Номенклатура
			ИмяПКОВидСубконто = "ВидыСубконтоТиповыеСтр";
