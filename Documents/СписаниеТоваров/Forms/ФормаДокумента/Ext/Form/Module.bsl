&НаКлиенте
Процедура ОткрытьСчетаНДС(Команда)
	
	Если НЕ ЭтаФорма.ТолькоПросмотр Тогда
		ЭтаФорма.ЗаблокироватьДанныеФормыДляРедактирования();
	КонецЕсли;
	
	СтруктураДанных = ПолучитьСтруктуру(Объект.Ссылка);
	
	СтруктураПараметров = Новый Структура;
	
	СтруктураПараметров.Вставить("СчетЗатратНДСБУ",              Объект.СчетЗатратНДСБУ);
	СтруктураПараметров.Вставить("СчетЗатратНДСНУ",              Объект.СчетЗатратНДСНУ);
	
	СтруктураПараметров.Вставить("СубконтоЗатратНДСБУ1",      	 Объект.СубконтоЗатратНДСБУ1);
	СтруктураПараметров.Вставить("СубконтоЗатратНДСБУ2",      	 Объект.СубконтоЗатратНДСБУ2);
	СтруктураПараметров.Вставить("СубконтоЗатратНДСБУ3",      	 Объект.СубконтоЗатратНДСБУ3);
	СтруктураПараметров.Вставить("СубконтоЗатратНДСНУ1",         Объект.СубконтоЗатратНДСНУ1);
	СтруктураПараметров.Вставить("СубконтоЗатратНДСНУ2",         Объект.СубконтоЗатратНДСНУ2);
	СтруктураПараметров.Вставить("СубконтоЗатратНДСНУ3",         Объект.СубконтоЗатратНДСНУ3);
	СтруктураПараметров.Вставить("Организация",           		 Объект.Организация);
	СтруктураПараметров.Вставить("СтруктурноеПодразделение",     Объект.СтруктурноеПодразделение);
	СтруктураПараметров.Вставить("Объект",     					 СтруктураДанных);
	
	СтруктураПараметров.Вставить("ПоказыватьВДокументахСчетаУчета", ЭтаФорма.ПоказыватьВДокументахСчетаУчета);
	СтруктураПараметров.Вставить("ВидимостьНалоговогоУчета",     	ЭтаФорма.ВидимостьНалоговогоУчета);
	СтруктураПараметров.Вставить("КорректироватьНДС",     	 		Объект.КорректироватьНДС);
	СтруктураПараметров.Вставить("УчитыватьНДС",     	 			Объект.УчитыватьНДС);
	
	ОткрытьФорму("Документ.СписаниеТоваров.Форма.ФормаНДС",СтруктураПараметров,ЭтаФорма);
	

КонецПроцедуры

