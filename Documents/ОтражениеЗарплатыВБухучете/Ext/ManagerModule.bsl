						Субконто = СтрокаДанных["СубконтоДтНУ" + СтрокаДанных.НомерСубконтоВидПлатежаДтНУ];
						Если НЕ ЗначениеЗаполнено(Субконто) Тогда
							СтрокаДанных["СубконтоДтНУ" + СтрокаДанных.НомерСубконтоВидПлатежаДтНУ] = СтрокаДанных.ВидПлатежа;
						КонецЕсли;
					КонецЕсли;
				КонецЕсли; 
			КонецЕсли;
			
		КонецЕсли;
		Если СтрокаДанных.СчетКтНУ <> ПустойСчетНУ Тогда
			Если СтрокаДанных.НомерСубконтоРаботникиОрганизацийКтНУ <> 0 Тогда
				Субконто = СтрокаДанных["СубконтоКтНУ" + СтрокаДанных.НомерСубконтоРаботникиОрганизацийКтНУ];
				Если НЕ ЗначениеЗаполнено(Субконто) Тогда
					Если СтрокаДанных.ВидОперации = ПредопределенноеЗначение("Перечисление.ВидыОперацийПоЗарплате.ВозмещениеУщерба")
						ИЛИ СтрокаДанных.ВидОперации = ПредопределенноеЗначение("Перечисление.ВидыОперацийПоЗарплате.УдержаниеПоПрочимОперациямСРаботниками") 
						ИЛИ СтрокаДанных.ВидОперации = ПредопределенноеЗначение("Перечисление.ВидыОперацийПоЗарплате.УдержаниеНеизрасходованныхПодотчетныхСумм") 
						ИЛИ СтрокаДанных.ВидОперации = ПредопределенноеЗначение("Перечисление.ВидыОперацийПоЗарплате.ВозвратИзлишнеВыплаченныхСуммВследствиеСчетныхОшибок") 
						ИЛИ СтрокаДанных.ВидОперации = ПредопределенноеЗначение("Перечисление.ВидыОперацийПоЗарплате.ПогашениеЗаймов") 
						ИЛИ СтрокаДанных.ВидОперации = ПредопределенноеЗначение("Перечисление.ВидыОперацийПоЗарплате.ПроцентыПоЗайму")
						ИЛИ СтрокаДанных.ВидОперации = ПредопределенноеЗначение("Перечисление.ВидыОперацийПоЗарплате.УдержаниеЗаОтпуск")
						ИЛИ СтрокаДанных.ВидОперации = ПредопределенноеЗначение("Перечисление.ВидыОперацийПоЗарплате.ПрочиеУдержания") Тогда
						СтрокаДанных["СубконтоКтНУ" + СтрокаДанных.НомерСубконтоРаботникиОрганизацийКтНУ] = СтрокаДанных.РаботникОрганизации;
					Иначе
						СтрокаДанных["СубконтоКтНУ" + СтрокаДанных.НомерСубконтоРаботникиОрганизацийКтНУ] = СтрокаДанных.ФизическоеЛицо;
					КонецЕсли;
				КонецЕсли;
			КонецЕсли;
			Если СтрокаДанных.НомерСубконтоКонтрагентыКтНУ <> 0 Тогда
				Субконто = СтрокаДанных["СубконтоКтНУ" + СтрокаДанных.НомерСубконтоКонтрагентыКтНУ];
				Если НЕ ЗначениеЗаполнено(Субконто) Тогда
					СтрокаДанных["СубконтоКтНУ" + СтрокаДанных.НомерСубконтоКонтрагентыКтНУ] = СтрокаДанных.Контрагент;
				КонецЕсли;
			КонецЕсли;
			Если СтрокаДанных.НомерСубконтоДоговорыКонтрагентовКтНУ <> 0 Тогда
				Субконто = СтрокаДанных["СубконтоКтНУ" + СтрокаДанных.НомерСубконтоДоговорыКонтрагентовКтНУ];
				Если НЕ ЗначениеЗаполнено(Субконто) Тогда
					СтрокаДанных["СубконтоКтНУ" + СтрокаДанных.НомерСубконтоДоговорыКонтрагентовКтНУ] = ОбщегоНазначенияБК.ДоступныйДоговорКонтрагента(СтрокаДанных.Контрагент["ОсновнойДоговорКонтрагента"]);
				КонецЕсли;
			КонецЕсли;
			Если СтрокаДанных.ВидОперации = ПредопределенноеЗначение("Перечисление.ВидыОперацийПоЗарплате.ОППВПеня")
				ИЛИ СтрокаДанных.ВидОперации = ПредопределенноеЗначение("Перечисление.ВидыОперацийПоЗарплате.ОПВРПеня")
				ИЛИ СтрокаДанных.ВидОперации = ПредопределенноеЗначение("Перечисление.ВидыОперацийПоЗарплате.ОПВПеня")
				ИЛИ СтрокаДанных.ВидОперации = ПредопределенноеЗначение("Перечисление.ВидыОперацийПоЗарплате.СОПеня")
				ИЛИ СтрокаДанных.ВидОперации = ПредопределенноеЗначение("Перечисление.ВидыОперацийПоЗарплате.ВОСМСПеня")
				ИЛИ СтрокаДанных.ВидОперации = ПредопределенноеЗначение("Перечисление.ВидыОперацийПоЗарплате.ООСМСПеня")
				ИЛИ СтрокаДанных.ВидОперации = ПредопределенноеЗначение("Перечисление.ВидыОперацийПоЗарплате.ЕППеня") Тогда
				Если СтрокаДанных.НомерСубконтоВидПлатежаКтНУ <> 0 Тогда
					Субконто = СтрокаДанных["СубконтоКтНУ" + СтрокаДанных.НомерСубконтоВидПлатежаКтНУ];
					Если НЕ ЗначениеЗаполнено(Субконто) Тогда
						СтрокаДанных["СубконтоКтНУ" + СтрокаДанных.НомерСубконтоВидПлатежаКтНУ] = СтрокаДанных.ВидПлатежа;
					КонецЕсли;
				КонецЕсли;
			КонецЕсли;
				
			Если ДанныеНалоговыйКомитет <> Неопределено И 
				(СтрокаДанных.СчетКтНУ = ПредопределенноеЗначение("ПланСчетов.Налоговый.ПрочиеНалоги") И ДанныеНалоговыйКомитет.ЗаполнятьНКпоСНВНУ) 
				ИЛИ (СтрокаДанных.СчетКтНУ = ПредопределенноеЗначение("ПланСчетов.Налоговый.ИндивидуальныйПодоходныйНалог") И ДанныеНалоговыйКомитет.ЗаполнятьНКпоИПНВНУ) Тогда
				
				НалоговыйКомитетВСтроке = ПроцедурыНалоговогоУчета.ПолучитьНалоговыйКомитетСтруктурнойЕдиницы(СтрокаДанных.СтруктурноеПодразделение, 
																							СтрокаДанных.Организация,
																							ДанныеНалоговыйКомитет.ИсчислениеНалоговСтруктурныхЕдиниц,
																							Перечисления.РазделыНалоговогоУчета.НалогиСЗаработнойПлаты);
				СтрокаДанных.СубконтоКтНУ3 = НалоговыйКомитетВСтроке;
				
			КонецЕсли;

		КонецЕсли;
		
	КонецЕсли;
	
КонецПроцедуры 

#КонецОбласти

&НаСервере
Процедура ЗаполнитьОборотПоПриобретеннымУслугамГПХВЦеляхНДС(СтруктураПараметров, АдресХранилища = Неопределено, Объект = Неопределено) Экспорт
	
	//УстановитьПривилегированныйРежим(Истина);

	Организация = СтруктураПараметров.Организация;
	ПериодРегистрации = СтруктураПараметров.ПериодРегистрации;
	ПлательщикНДС = ПроцедурыНалоговогоУчета.ПолучитьПризнакПлательщикаНДС(Организация, ПериодРегистрации);

	Если НЕ ПлательщикНДС Тогда
		Возврат;
	КонецЕсли;
	
	Если УчетнаяПолитикаСервер.ПолучитьМетодОтнесенияНДСВЗачет(Организация, ПериодРегистрации) = Перечисления.МетодыОтнесенияНДСВЗачет.Пропорциональный Тогда
		НДСВидОборота = Перечисления.ВидыОборотовПоРеализации.Общий;
	Иначе
		НДСВидОборота = Перечисления.ВидыОборотовПоРеализации.Облагаемый;
	КонецЕсли;

	ТаблицаОборотПоПриобретеннымУслугамГПХВЦеляхНДС = Новый ТаблицаЗначений;
	
	Запрос = Новый Запрос;
		
	Запрос.УстановитьПараметр("НачалоПериода", НачалоМесяца(ПериодРегистрации));
	Запрос.УстановитьПараметр("КонецПериода", КонецМесяца(ПериодРегистрации));
	Запрос.УстановитьПараметр("Организация", Организация);
	Запрос.УстановитьПараметр("СтавкаБезНДС", Справочники.СтавкиНДС.ПолучитьСтавкуБезНДС(Ложь));
	
	ТекстЗапроса =
		"ВЫБРАТЬ
		|	ОтражениеЗарплатыВБухучетеНачисленнаяЗарплатаИВзносы.Сумма КАК Сумма,
		|	ОтражениеЗарплатыВБухучетеНачисленнаяЗарплатаИВзносы.Контрагент КАК Контрагент,
		|	ОтражениеЗарплатыВБухучетеНачисленнаяЗарплатаИВзносы.ДоговорКонтрагента КАК ДоговорКонтрагента,
		|	&СтавкаБезНДС КАК СтавкаНДС,
		|	ЗНАЧЕНИЕ(Справочник.ВидыПоступления.ПриобретениеТМЗБезНДС) КАК НДСВидПоступления,
		|	&НДСВидОборота КАК НДСВидОборота
		|ИЗ
		|	Документ.ОтражениеЗарплатыВБухучете.НачисленнаяЗарплатаИВзносы КАК ОтражениеЗарплатыВБухучетеНачисленнаяЗарплатаИВзносы
		|ГДЕ
		|	ОтражениеЗарплатыВБухучетеНачисленнаяЗарплатаИВзносы.Ссылка = &Ссылка
		|	И ОтражениеЗарплатыВБухучетеНачисленнаяЗарплатаИВзносы.ВидОперации = &ВидОперацииДоходыКонтрагентов
		|	И ОтражениеЗарплатыВБухучетеНачисленнаяЗарплатаИВзносы.Контрагент <> ЗНАЧЕНИЕ(Справочник.Контрагенты.ПустаяСсылка)";
	
	Запрос.Текст = ТекстЗапроса;
	Запрос.УстановитьПараметр("Ссылка", СтруктураПараметров.Ссылка);
	Запрос.УстановитьПараметр("НДСВидОборота", НДСВидОборота);
	Запрос.УстановитьПараметр("ВидОперацииДоходыКонтрагентов", Перечисления.ВидыОперацийПоЗарплате.ДоходыКонтрагентов);
	ТаблицаОборотПоПриобретеннымУслугамГПХВЦеляхНДС = Запрос.Выполнить().Выгрузить();
	
	Если СтруктураПараметров.Загрузка Тогда
		//Объект = СтруктураПараметров.Ссылка.ПолучитьОбъект();
		Объект.ОборотПоПриобретеннымУслугамГПХВЦеляхНДС.Загрузить(ТаблицаОборотПоПриобретеннымУслугамГПХВЦеляхНДС);
	Иначе

		СтруктураДанныхЗаполнения = Новый Структура;
		//УстановитьПривилегированныйРежим(Ложь);
		СтруктураДанныхЗаполнения.Вставить("ТаблицаОборотПоПриобретеннымУслугамГПХВЦеляхНДС", ТаблицаОборотПоПриобретеннымУслугамГПХВЦеляхНДС);
		ПоместитьВоВременноеХранилище(СтруктураДанныхЗаполнения, АдресХранилища);
 	КонецЕсли;
				
КонецПроцедуры

