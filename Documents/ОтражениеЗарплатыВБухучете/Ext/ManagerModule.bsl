    |   ВЫБОР
    |       КОГДА (ВТЗаполненнаяСтатьяПассивов.СчетУчета ЕСТЬ NULL
    |               ИЛИ ВТЗаполненнаяСтатьяПассивов.СчетУчета = ЗНАЧЕНИЕ(ПланСчетов.Типовой.ПустаяСсылка))
    |               И ВТСчетаПоОперациям.АналитикаПоРаботникам 
    |               И НезаполненнаяАналитика.ВидОперации <> ЗНАЧЕНИЕ(Перечисление.ВидыОперацийПоЗарплате.ДобровольныеВзносыВНПФ) 
    |           ТОГДА НезаполненнаяАналитика.ФизическоеЛицо
    |       КОГДА (ВТЗаполненнаяСтатьяПассивов.СчетУчета ЕСТЬ NULL
    |               ИЛИ ВТЗаполненнаяСтатьяПассивов.СчетУчета = ЗНАЧЕНИЕ(ПланСчетов.Типовой.ПустаяСсылка))
    |               И ВТСчетаПоОперациям.АналитикаПоКонтрагентам
    |           ТОГДА ВЫБОР
    |                   КОГДА НезаполненнаяАналитика.Контрагент <> НЕОПРЕДЕЛЕНО
    |                       ТОГДА НезаполненнаяАналитика.Контрагент
    |                   ИНАЧЕ ЗНАЧЕНИЕ(Справочник.Контрагенты.ПустаяСсылка)
    |               КОНЕЦ
    |       ИНАЧЕ ВТЗаполненнаяСтатьяПассивов.Субконто1
    |   КОНЕЦ КАК Субконто1,
    |   ВЫБОР
    |       КОГДА (ВТЗаполненнаяСтатьяПассивов.СчетУчета ЕСТЬ NULL
    |               ИЛИ ВТЗаполненнаяСтатьяПассивов.СчетУчета = ЗНАЧЕНИЕ(ПланСчетов.Типовой.ПустаяСсылка))
    |               И ВТСчетаПоОперациям.АналитикаПоКонтрагентам
    |           ТОГДА ЗНАЧЕНИЕ(Справочник.ДоговорыКонтрагентов.ПустаяСсылка)
    |       ИНАЧЕ ЕСТЬNULL(ВТЗаполненнаяСтатьяПассивов.Субконто2, ЗНАЧЕНИЕ(Справочник.ДоговорыКонтрагентов.ПустаяСсылка))
    |   КОНЕЦ КАК Субконто2,
    |   ВЫБОР
    |       КОГДА (ВТЗаполненнаяСтатьяПассивов.СчетУчета ЕСТЬ NULL
    |               ИЛИ ВТЗаполненнаяСтатьяПассивов.СчетУчета = ЗНАЧЕНИЕ(ПланСчетов.Типовой.ПустаяСсылка))
    |               И ВТСчетаПоОперациям.АналитикаПоРаботникам 
    |               И НезаполненнаяАналитика.ВидОперации = ЗНАЧЕНИЕ(Перечисление.ВидыОперацийПоЗарплате.ДобровольныеВзносыВНПФ)
    |           ТОГДА НезаполненнаяАналитика.ФизическоеЛицо
    |       ИНАЧЕ ВТЗаполненнаяСтатьяПассивов.Субконто3
    |   КОНЕЦ КАК Субконто3
    |ПОМЕСТИТЬ ЗаполненнаяСтатьяПассивов
    |ИЗ
    |   ВТНезаполненнаяАналитика КАК НезаполненнаяАналитика
    |       ЛЕВОЕ СОЕДИНЕНИЕ ВТЗаполненнаяСтатьяПассивов КАК ВТЗаполненнаяСтатьяПассивов
    |       ПО НезаполненнаяАналитика.ВидОперации = ВТЗаполненнаяСтатьяПассивов.ВидОперации
    |           И НезаполненнаяАналитика.ФизическоеЛицо = ВТЗаполненнаяСтатьяПассивов.ФизическоеЛицо
    |           И НезаполненнаяАналитика.Подразделение = ВТЗаполненнаяСтатьяПассивов.Подразделение
    |           И НезаполненнаяАналитика.Контрагент = ВТЗаполненнаяСтатьяПассивов.Контрагент
    |       ВНУТРЕННЕЕ СОЕДИНЕНИЕ ВТСчетаПоОперациям КАК ВТСчетаПоОперациям
    |       ПО НезаполненнаяАналитика.ВидОперации = ВТСчетаПоОперациям.ВидОперации";
	
	Если ИмяТаблицы = "" Тогда
		ТекстЗапроса = ТекстЗапросаУдержаннаяЗарплата;
	ИначеЕсли ИмяТаблицы = "УдержаннаяЗарплата" Тогда
		ТекстЗапроса = ТекстЗапросаУдержаннаяЗарплата;
	КонецЕсли;
	
	Запрос = Новый Запрос(ТекстЗапроса);
	Запрос.МенеджерВременныхТаблиц = МенеджерВременныхТаблиц;
	Запрос.УстановитьПараметр("ТекущийДокумент", ТекущийДокумент);
	Запрос.УстановитьПараметр("ДатаАктуальности", ДатаАктуальности);
	Запрос.Выполнить();
	
КонецПроцедуры

Функция ПредставлениеОтраженияВУчете(ОтражениеВУчете) Экспорт
	
	Результат = НСтр("ru = 'Настроить'");
	
    Если Не ЗначениеЗаполнено(ОтражениеВУчете.СчетУчета) Тогда
    	Возврат Результат;
    КонецЕсли;
    
    МассивПолей = Новый Массив;
    МассивПолей.Добавить(Строка(ОтражениеВУчете.СчетУчета));
    
    СвойстваСчета = ПроцедурыБухгалтерскогоУчетаВызовСервераПовтИсп.ПолучитьСвойстваСчета(ОтражениеВУчете.СчетУчета);
    Для НомерСубконто = 1 По СвойстваСчета.КоличествоСубконто Цикл
    	ЗначениеСубконто = ОтражениеВУчете["Субконто" + НомерСубконто];
    	
    	Если ЗначениеЗаполнено(ЗначениеСубконто) Тогда
    		МассивПолей.Добавить(Строка(ЗначениеСубконто));
    	КонецЕсли;
    	
    КонецЦикла;
    
    Результат = СтрСоединить(МассивПолей, ", ");
    
    Возврат Результат;
	
КонецФункции

Процедура ЗаполнитьПредставлениеСчетаУчетаВТЧ(ТаблицаФормы) Экспорт
	
	Для каждого СтрокаТаблицы Из ТаблицаФормы Цикл
		
		СтрокаТаблицы.ПредставлениеОтраженияВУчете = Документы.ОтражениеЗарплатыВБухучете.ПредставлениеОтраженияВУчете(СтрокаТаблицы);
		
	КонецЦикла;
	
КонецПроцедуры

